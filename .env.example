# AI Model Configuration
# OpenAI API Key (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=your_openai_key_here

# Anthropic API Key (get from https://console.anthropic.com/)
ANTHROPIC_API_KEY=your_anthropic_key_here

# Model Selection (format: provider:model_name)
# Examples: openai:gpt-4o-mini, anthropic:claude-3-sonnet-20240229, ollama:llama3.1
DEFAULT_MODEL=openai:gpt-4o-mini
PLANNER_MODEL=openai:gpt-4o-mini
SOLVER_MODEL=openai:gpt-4o-mini
DEFAULT_TEMPERATURE=0

# Ollama Configuration (for local models)
# Install Ollama from https://ollama.ai
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1

# Agent Configuration
MAX_ITERATIONS=10
LOG_LEVEL=INFO

# Backend Configuration
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# Stage Configuration (1, 2, 3.1, 4.1)
STAGE = 2

# Enable agent checkpointing to maintain state
ENABLE_CHECKPOINTING=false